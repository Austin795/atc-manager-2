{
  "private": true,
  "name": "simple-preact-pwa",
  "homepage": "",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "set NODE_ENV=production && preact build -p --json --prerender --clean",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "history": "^4.7.2",
    "lz-string": "^1.4.4",
    "milligram": "^1.3.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.6.1"
  }
}
